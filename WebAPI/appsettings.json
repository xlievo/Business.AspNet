{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "WebSockets": {
    "KeepAliveInterval": 120,
    "ReceiveBufferSize": 4096,
    "AllowedOrigins": []
  },
  "FormOptions": {
    "KeyLengthLimit": 2048,
    "ValueCountLimit": 2048
  },
  "Kestrel": {
    "AllowSynchronousIO": false,
    "Limits": {
      "MinRequestBodyDataRate": null
    }
  }
}